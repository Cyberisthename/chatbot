{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DEMO_MODE": "true",
    "API_HOST": "0.0.0.0",
    "API_PORT": "3001",
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "regions": ["iad1"],
  "alias": ["jarvis-2v-demo"]
}